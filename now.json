{
  "name": "TravelMasterPlan",
  "version": 2,
  "builds": [{ "src": "*/index.js", "use": "@now/node" }],
  "routes": [{ "src": "/(.*)", "dest": "/src/index.js" }],
  "env": {
     "DB_USER": "@db_user",
     "DB_PASSWORD" :"@db_password",
     "DB_HOST":"@db_host",
     "DB_NAME":"@db_name"
  }


}